{
  "name": "blobber3",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "fix": "standard --parser babel-eslint --fix",
    "buildClient": "webpack --config config/webpack.config.client.js",
    "watchClient": "nodemon -w src/client/ --exec \"yarn buildClient\"",
    "buildServer": "webpack --config config/webpack.config.server.js",
    "watchServer": "nodemon -w src/server/ --exec \"yarn buildServer; node dist/server.js\"",
    "dev": "concurrently -n 'server,client,linter' --kill-others-on-fail \"yarn watchServer\" \"yarn watchClient\" \"standard --parser babel-eslint\" "
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "path": "^0.12.7",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rxjs": "^5.5.10",
    "socket.io": "^2.1.0"
  }
}
